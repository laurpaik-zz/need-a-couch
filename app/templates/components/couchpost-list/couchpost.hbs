<li class="list-group-item">
  {{#unless profile.givenName}}
    {{#if couchpost.couchFound}}
      User {{profile.id}} found a couch near {{couchpost.location}} for {{couchpost.dateNeeded}}!
      {{#if couchpost.editable}}
        {{#link-to 'couchpost.edit' couchpost class="btn btn-xs btn-primary"}} Edit {{/link-to}}
        <button class="btn btn-xs btn-warning" {{action 'delete'}}>Delete</button>
      {{/if}}
    {{/if}}
    {{#unless couchpost.couchFound}}
      User {{profile.id}} needs a couch near {{couchpost.location}} on {{couchpost.dateNeeded}}!
      {{#if couchpost.editable}}
        {{#link-to 'couchpost.edit' couchpost class="btn btn-xs btn-primary"}} Edit {{/link-to}}
        <button class="btn btn-xs btn-warning" {{action 'delete'}}>Delete</button>
      {{/if}}
    {{/unless}}
  {{/unless}}
  {{#if profile.givenName}}
    {{#if couchpost.couchFound}}
      {{profile.givenName}} {{profile.surname}} found a couch near {{couchpost.location}} for {{couchpost.dateNeeded}}!
      {{#if couchpost.editable}}
        {{#link-to 'couchpost.edit' couchpost class="btn btn-xs btn-primary"}} Edit {{/link-to}}
        <button class="btn btn-xs btn-warning" {{action 'delete'}}>Delete</button>
      {{/if}}
    {{/if}}
    {{#unless couchpost.couchFound}}
      {{profile.givenName}} {{profile.surname}} needs a couch near {{couchpost.location}} on {{couchpost.dateNeeded}}!
      {{#if couchpost.editable}}
        {{#link-to 'couchpost.edit' couchpost class="btn btn-xs btn-primary"}} Edit {{/link-to}}
        <button class="btn btn-xs btn-warning" {{action 'delete'}}>Delete</button>
      {{/if}}
    {{/unless}}
  {{/if}}
</li>
